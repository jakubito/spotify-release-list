    "fuse.js": "^6.6.2",
    "js-base64": "^3.7.2",
    "localforage": "^1.10.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.3",
    "query-string": "^7.1.1",
    "react": "^18.1.0",
    "react-colorful": "^5.5.1",
    "react-dates": "^21.8.0",
    "react-dom": "^18.1.0",
    "react-hook-form": "^7.31.1",
    "react-hotkeys-hook": "^3.4.4",
    "react-ios-pwa-prompt": "^1.8.4",
    "react-redux": "^8.0.1",
    "react-responsive": "^9.0.0-beta.8",
    "react-router-dom": "^6.3.0",
    "react-scripts": "^5.0.1",
    "react-waypoint": "^10.1.0",
